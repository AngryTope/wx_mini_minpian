<view class="page">
  <view class="headerImgView">
    <image id="headerImg" src="{{urlHeaderImg}}" mode="{{imageMode}}"></image>
  </view>
  <view class="good">
    <scroll-view scroll-x upper-threshold="50" class="scrollView" >
      <block wx:for="{{tabList}}">
        <view class="swiperTabList {{currentIndex==item.index ? 'on' : ''}}" bindtap="swichNav">{{item.tabtext}}</view>
      </block>
    </scroll-view>
    <view class="goodList">

    </view>
  </view>
</view>